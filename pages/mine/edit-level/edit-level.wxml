<!--pages/mine/edit-level/edit-level.wxml-->
<view class='b-btn'>
  <button type='primary' hover-class='active' bindtap='submit'>确定</button>
</view>
<view class="weui-cells">
  <view class="weui-cell weui-cell_select">
    <view class="weui-cell__bd">
      <picker bindchange="bindLevelChange" value="{{levelIndex}}" range="{{levels}}" range-key='label'>
        <view class="weui-select">{{levels[levelIndex].label}}</view>
      </picker>
    </view>
  </view>
</view>
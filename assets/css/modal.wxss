/*评价  */

.modal {
  position: fixed;
  z-index: 99;
  width: 100%;
  height: 100;
  top: 0;
  left: 0;
  display: flex;
  justify-content: center;
}

.modal-bg {
  position: absolute;
  z-index: -1;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.26);
}

.modal-assess {
  width: 80%;
  height: 400rpx;
  background-color: #fff;
  border-radius: 8rpx;
}
